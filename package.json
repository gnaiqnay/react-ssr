{
    "name": "react-scaffolding",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "start": "cross-env NODE_ENV=development webpack serve --config ./webpack.config.dev.js --open",
        "build": "cross-env NODE_ENV=prodction webpack --config ./webpack.config.prod.js",
        "lint:js": "eslint src/ --ext .js,.jsx,.ts,.tsx --fix",
        "lint:style": "stylelint \"src/**/*.{scss,css}\" --cache --fix",
        "lint:*": "npm run lint:js && npm run lint:style",
        "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\""
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "src/**/*.{js,jsx}": [
            "eslint  --fix",
            "prettier --write"
        ],
        "src/**/*.{css,less,scss}": [
            "stylelint --cache --fix"
        ]
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/yqiang2019/react-scaffolding.git"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/yqiang2019/react-scaffolding/issues"
    },
    "homepage": "https://github.com/yqiang2019/react-scaffolding#readme",
    "dependencies": {
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-router-dom": "^6.21.1"
    },
    "devDependencies": {
        "@babel/core": "^7.23.6",
        "@babel/preset-env": "^7.23.6",
        "@babel/preset-react": "^7.23.3",
        "@babel/preset-typescript": "^7.23.3",
        "@types/react-dom": "^18.2.18",
        "@typescript-eslint/eslint-plugin": "^6.15.0",
        "@typescript-eslint/parser": "^6.15.0",
        "babel-loader": "^9.1.3",
        "core-js": "^3.34.0",
        "cross-env": "^7.0.3",
        "css-loader": "^6.8.1",
        "eslint": "^8.56.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-prettier": "^5.1.1",
        "eslint-plugin-react": "^7.33.2",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-webpack-plugin": "^4.0.1",
        "html-webpack-plugin": "^5.6.0",
        "husky": "^8.0.3",
        "lint-staged": "^15.2.0",
        "mini-css-extract-plugin": "^2.7.6",
        "prettier": "^3.1.1",
        "sass": "^1.69.5",
        "sass-loader": "^13.3.2",
        "style-loader": "^3.3.3",
        "stylelint": "^16.0.2",
        "stylelint-config-standard": "^35.0.0",
        "typescript": "^5.3.3",
        "webpack": "^5.89.0",
        "webpack-cli": "^5.1.4",
        "webpack-dev-server": "^4.15.1",
        "webpack-merge": "^5.10.0"
    }
}
